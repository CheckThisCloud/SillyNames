{
  "name": "checkthiscloud/silly-names",
  "description": "Generate silly names by combining adjectives and subjects with support for multiple languages",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "keywords": ["names", "generator", "silly", "random", "czech", "english", "slovak"],
  "autoload": {
    "psr-4": {
      "CheckThisCloud\\SillyNames\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CheckThisCloud\\SillyNames\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0|^12.0",
    "phpunit/php-code-coverage": "*",
    "phpstan/phpstan": "^2.0"
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse",
    "check": [
      "@analyse",
      "@test"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
