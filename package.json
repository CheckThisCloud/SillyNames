{
  "name": "sillynames",
  "version": "1.0.0",
  "description": "A production-quality TypeScript library for generating silly, amusing, and memorable names",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "dev": "npm run build:watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint",
    "prepack": "npm run build"
  },
  "keywords": [
    "silly",
    "names",
    "generator",
    "random",
    "funny",
    "amusing",
    "typescript",
    "library",
    "production"
  ],
  "author": "CheckThisCloud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CheckThisCloud/SillyNames.git"
  },
  "bugs": {
    "url": "https://github.com/CheckThisCloud/SillyNames/issues"
  },
  "homepage": "https://github.com/CheckThisCloud/SillyNames#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
